{
  "expo": {
    "name": "Banky",
    "slug": "banky",
    "description": "A financial assistant app",
    "githubUrl": "https://github.com/rodcordeiro/banky",
    "backgroundColor": "#8A2BE2",
    "primaryColor": "#8A2BE2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "privacy": "public",
    "owner": "rodcordeiro",
    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#8A2BE2"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/64a19d11-6df9-40e6-b7ef-d5b6f893c294"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#8A2BE2"
      },
      "package": "com.rodcordeiro.banky",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "64a19d11-6df9-40e6-b7ef-d5b6f893c294"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": ["sentry-expo"],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "rodcordeiro",
            "project": "banky"
          }
        }
      ]
    }
  }
}
